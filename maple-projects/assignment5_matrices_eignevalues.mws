{MPLTEXT 1 0 715 "with(LinearAlgebra);\n\n\nfor i from 1 to 8 do\n    \+
\n    M := RandomMatrix(3, 3);\n    \n    \n    eig_vals := Eigenvalue
s(M);\n    \n    \n    prod_eig := mul(eig_vals[j], j = 1 .. nops(eig_
vals));\n    \n    \n    simplified_prod := simplify(expand(prod_eig))
;\n    \n    \n    det_M := Determinant(M);\n    \n    \n    print(\"M
atrix M:\", M);\n    print(\"Eigenvalues:\", eig_vals);\n    print(\"P
roduct of Eigenvalues (simplified):\", simplified_prod);\n    print(\"
Determinant of M:\", det_M);\n    \n    \n    if simplified_prod = det
_M then\n        print(\"Product of eigenvalues equals determinant for
 matrix \", i);\n    else\n        print(\"Mismatch for matrix \", i);
\n    end if;\n    \n    print(\"---------------------------------\");
